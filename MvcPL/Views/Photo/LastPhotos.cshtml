@model IEnumerable<MvcPL.ViewModels.PhotoViewModel>

<div class="div-table" id="search-photos">
    <div class="div-row">
        @foreach (var photo in Model)
            {
            <div class="div-cell">
                <img src='/images/@photo.Id' width="100" height="100" />
            </div>
        }
    </div>
</div>

